<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">

  <title>NITAP Placements</title>
  <meta content="" name="description">
  <meta content="" name="keywords">

  <!-- Favicons -->
  <link href="images/nit-logo.png" rel="icon">

  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Jost:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

  <!-- Vendor CSS Files -->
  <link rel="stylesheet" href="aos/aos.css">
  <link rel="stylesheet" href="bootstrap/css/bootstrap.min.css">
  <link rel="stylesheet" href="bootstrap-icons/bootstrap-icons.css">
  <link rel="stylesheet" href="boxicons/css/boxicons.min.css">
  <link rel="stylesheet" href="glightbox/css/glightbox.min.css">
  <link rel="stylesheet" href="remixicon/remixicon.css">
  <link rel="stylesheet" href="swiper/swiper-bundle.min.css">

  <!-- Template Main CSS File -->
  <link href="css/index.css" rel="stylesheet">
</head>

<body>

  <!-- ======= Header ======= -->
  <header id="header" class="fixed-top " style="background-color: #37517e;">
    <div class="container d-flex align-items-center">
      <a href="/" class="logo me-auto"><img src="images/nit-logo.png" alt="" class="img-fluid"></a>
      <h1 class="logo me-auto"><a href="/">Placements NIT Andhra Pradesh</a></h1>

      <nav id="navbar" class="navbar">
        <ul>
          <li><a class="nav-link scrollto active" href="/">Home</a></li>
          <li><a class="nav-link scrollto" href="/companies">Companies</a></li>
          <li><a class="nav-link scrollto" href="/stories">Placement Stories</a></li>
          <li><a class="nav-link scrollto" href="/contact">Contact Us</a></li>
          <li><a class="nav-link scrollto" href="/login">T&P Login</a></li>
        </ul>
        <i class="bi bi-list mobile-nav-toggle"></i>
      </nav><!-- .navbar -->

    </div>
  </header><!-- End Header -->

<br>
<br><br>
<section id="services" class="services sections-bg" >
 <div class="container" data-aos="fade-up">
    <div class="row gy-4" data-aos="fade-up" data-aos-delay="100">
      <% Member.forEach(function(member){ %>
        <div class="col-lg-6" data-aos="zoom-in" data-aos-delay="100">
          <div class="student d-flex align-items-start">
            <div class="pic"><img src="/images/contact/avatarmen.png" class="img-fluid" alt=""></div>
            <div class="student-info">
              <h4><%= member.Name %></h4>
              <span>Mail: <%= member.Mail_id %></span>
              <p>Department:  <%= member.Department %></p>
            </div>
          </div>
        </div>
      <% }); %>
    </div>
  </div>
</section>


<%- include("partials/main/footer"); -%>